# Voice Synthesis APIs
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
PLAYHT_API_KEY=your_playht_api_key_here
MURF_API_KEY=your_murf_api_key_here
RESEMBLE_API_KEY=your_resemble_api_key_here
DESCRIPT_API_KEY=your_descript_api_key_here

# Text/LLM APIs
ANTHROPIC_API_KEY=your_anthropic_api_key_here  # For Claude 3
GOOGLE_API_KEY=your_google_api_key_here  # For Gemini
REPLICATE_API_KEY=your_replicate_api_key_here  # For Llama 3

# Motion Capture & Avatar APIs
WONDER_API_KEY=your_wonder_dynamics_api_key_here
DEEPMOTION_API_KEY=your_deepmotion_api_key_here
MOVEAI_API_KEY=your_moveai_api_key_here
HEYGEN_API_KEY=your_heygen_api_key_here
DID_API_KEY=your_did_api_key_here
