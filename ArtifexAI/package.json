{
  "name": "artifex-ai",
  "version": "1.0.0",
  "description": "Artifex.AI - Professional Node-Based AI Video Production Suite",
  "main": "dist/main.js",
  "author": "Artifex Team",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\" \"npm run dev:server\"",
    "dev:main": "cross-env NODE_ENV=development webpack --config webpack.main.config.js --watch",
    "dev:renderer": "cross-env NODE_ENV=development webpack serve --config webpack.renderer.config.js",
    "dev:server": "python server/app.py",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack.main.config.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.config.js",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:win": "electron-builder --win",
    "dist:mac": "electron-builder --mac",
    "dist:linux": "electron-builder --linux",
    "postinstall": "electron-builder install-app-deps",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\""
  },
  "build": {
    "appId": "com.artifex.ai.studio",
    "productName": "Artifex.AI",
    "directories": {
      "output": "dist-app"
    },
    "files": [
      "dist/**/*",
      "assets/**/*",
      "server/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "server",
        "to": "server"
      }
    ],
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.ico",
      "forceCodeSigning": false,
      "signDlls": false,
      "requestedExecutionLevel": "asInvoker",
      "verifyUpdateCodeSignature": false,
      "publisherName": "Artifex Team"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Artifex.AI",
      "perMachine": false,
      "differentialPackage": false
    },
    "mac": {
      "category": "public.app-category.video",
      "icon": "assets/icon.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "assets/icon.png",
      "category": "Video"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@reduxjs/toolkit": "^2.2.3",
    "@xyflow/react": "^12.0.0",
    "axios": "^1.6.8",
    "cytoscape": "^3.28.1",
    "cytoscape-dagre": "^2.5.0",
    "d3": "^7.9.0",
    "dagre": "^0.8.5",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.8",
    "elkjs": "^0.9.2",
    "framer-motion": "^11.18.2",
    "konva": "^9.3.6",
    "litegraph.js": "^0.7.18",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-flow-renderer": "^10.3.17",
    "react-konva": "^18.2.10",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "reactflow": "^11.11.3",
    "recharts": "^2.12.5",
    "rete": "^2.0.3",
    "rete-react-plugin": "^2.0.3",
    "socket.io-client": "^4.7.5",
    "uuid": "^9.0.1",
    "wavesurfer.js": "^7.7.11"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@types/cytoscape": "^3.19.16",
    "@types/d3": "^7.4.3",
    "@types/dagre": "^0.7.52",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.1",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-packager": "^17.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-node-externals": "^3.0.0"
  }
}
